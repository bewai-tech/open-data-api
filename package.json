{
    "name": "opendata-api",
    "version": "1.0.0",
    "main": "app.js",
    "engines": {
        "node": ">=16.0.0",
        "npm": ">=7.0.0"
    },
    "engineStrict": true,
    "scripts": {
        "initTables": "ts-node-dev src/utils/initTables.ts",
        "indexTable": "ts-node-dev src/utils/indexTable.ts",
        "importData": "ts-node-dev src/utils/importData.ts --",
        "downloadAssets": "ts-node-dev src/utils/downloadAssets.ts -- ",
        "install:ci": "npm ci --loglevel=error --cache .npm --prefer-offline --no-audit",
        "install:ci:light": "npm ci --silent --no-audit",
        "start": "ts-node-dev src/app.ts --max_old_space_size=2048",
        "test:unit": "node --trace-warnings --unhandled-rejections=strict node_modules/.bin/jest --maxWorkers=1 -c jest.unit.config.js",
        "test:unit:ci": "npm run test:unit -- --ci --coverage --reporters=default --reporters=jest-junit",
        "lint": "eslint src/ --ext .ts",
        "clean": "rm -rf dist",
        "build": "npm run clean && tsc && npm run copy-files",
        "copy-files": "cp -R ./src/cmd ./dist",
        "commit": "cz",
        "prepare": "is-ci || husky install"
    },
    "repository": {
        "type": "git",
        "url": "git+ssh://git@gitlab.com/easy-chain/opensource/opendataapi.git"
    },
    "author": "Bewai",
    "license": "MIT",
    "config": {
        "commitizen": {
            "path": "cz-conventional-changelog"
        }
    },
    "homepage": "https://gitlab.com/easy-chain/opensource/opendataapi",
    "devDependencies": {
        "@types/cli-progress": "^3.11.0",
        "@types/compression": "^1.7.2",
        "@types/cors": "^2.8.13",
        "@types/express": "^4.17.17",
        "@types/helmet": "^4.0.0",
        "@types/jest": "^29.2.4",
        "@types/lodash": "^4.14.195",
        "@types/morgan": "^1.9.5",
        "@types/node": "^18.11.13",
        "@types/pg-copy-streams": "^1.2.2",
        "@types/pg-format": "^1.0.2",
        "@types/progress-stream": "^2.0.2",
        "@typescript-eslint/eslint-plugin": "^5.46.0",
        "@typescript-eslint/parser": "^5.46.0",
        "commitizen": "^4.2.6",
        "cz-conventional-changelog": "^3.3.0",
        "eslint": "^8.29.0",
        "husky": "^8.0.2",
        "jest": "^29.3.1",
        "jest-environment-jsdom": "^29.3.1",
        "jest-junit": "^15.0.0",
        "ts-jest": "^29.0.3",
        "ts-node-dev": "^2.0.0"
    },
    "dependencies": {
        "@fast-csv/parse": "^4.3.6",
        "@sentry/node": "^7.24.2",
        "@sentry/tracing": "^7.24.2",
        "@types/pg": "^8.10.2",
        "axios": "^1.4.0",
        "body-parser": "^1.20.2",
        "cli-progress": "^3.12.0",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "express-async-handler": "^1.2.0",
        "helmet": "^7.0.0",
        "is-ci": "^3.0.1",
        "lodash": "^4.17.21",
        "morgan": "^1.10.0",
        "pg": "^8.11.0",
        "pg-copy-streams": "^6.0.5",
        "pg-format": "^1.0.4",
        "pg-promise": "^11.5.0",
        "progress-stream": "^2.0.0",
        "typescript": "^4.9.4",
        "winston": "^3.8.2",
        "winston-daily-rotate-file": "^4.7.1"
    }
}